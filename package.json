{
  "name": "trading-terminal",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "scripts": {
    "dev": "concurrently --names proxy,web --prefix-colors blue,green \"pnpm --filter proxy dev\" \"pnpm --filter web dev\"",
    "sync": "pnpm --filter scripts sync:pbt",
    "seed:feb13": "pnpm --filter scripts seed",
    "seed:feb14": "pnpm --filter scripts seed:feb14"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
